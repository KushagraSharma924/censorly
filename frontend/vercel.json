{
  "version": 2,
  "functions": {
    "app.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "/public/$1/$2",
      "headers": {
        "cache-control": "s-maxage=31536000"
      }
    },
    {
      "src": "/processed/(.*)",
      "dest": "/public/processed/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "BACKEND_URL": "https://ai-profanity-filter.onrender.com"
  }
}